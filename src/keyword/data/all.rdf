  <rdf:Description rdf:about="https://nakamura196.github.io/hi_person/term/keyword/大炊頭.json">
    <ns1:description>大炊寮おおいりようの長官。従五位下相当。</ns1:description>
    <ns1:url rdf:resource="https://kotobank.jp/word/大炊頭"/>
